\newcommand{\Hlxnewtheorem}[3]{%
  \newenvironment{#1}{\begin{quotation}\textbf{#2.}
      \begin{it}}{\end{it}\end{quotation}}}

\newenvironment{blockquote}%
  {\endpar\HlxBlk\HlxModeLevelUp\xml{blockquote}\HlxFirstPar}
  {\HlxBlk\HlxLeavePar\xml{/blockquote}\HlxModeLevelDown}
\newenvironment{verse}
  {\endpar\HlxBlk\HlxModeLevelUp\xml{div class="verse"}\HlxFirstPar}
  {\endpar\HlxLeavePar\xml{/div}\HlxModeLevelDown}
\newenvironment{quote}
  {\endpar\HlxBlk\HlxModeLevelUp\xml{div class="quote"}\HlxFirstPar}
  {\endpar\HlxLeavePar\xml{/div}\HlxModeLevelDown}
\newenvironment{quotation}
  {\endpar\HlxBlk\HlxModeLevelUp\xml{div class="quotation"}\HlxFirstPar}
  {\endpar\HlxLeavePar\xml{/div}\HlxModeLevelDown}

\newenvironment{center}
  {\endpar\HlxBlk\HlxModeLevelUp\xml{div align="center"}\HlxFirstPar\HlxBlk}
  {\endpar\HlxBlk\HlxLeavePar\xml{/div}\HlxModeLevelDown\HlxBlk}

\newcounter{listcounter}\setcounter{listcounter}{0}

\newenvironment{menu}{\endpar\suspendpars%
  \HlxBlk\HlxModeLevelUp\xml{ul class="menu"}%
  \begingroup\stepcounter{listcounter}%
  \newcounter{templistcounter}%
  \newcommand{\item}{\HlxEnterVmode%
    \begin{ifequal}{\thetemplistcounter}{\thelistcounter}%
      \HlxLeavePar\xml{/li}\end{ifequal}%
      \setcounter{templistcounter}{\thelistcounter}%
      \HlxBlk\xml{li}\HlxFirstPar}\ignorespaces}%
    {\begin{ifequal}{\thetemplistcounter}{\thelistcounter}%
        \HlxLeavePar\xml{/li}\end{ifequal}\endgroup%
    \HlxBlk\xml{/ul}\HlxModeLevelDown\resumepars}

\newenvironment{itemize}{\endpar\suspendpars%
  \HlxBlk\HlxModeLevelUp\xml{ul}%
  \begingroup\stepcounter{listcounter}%
  \newcounter{templistcounter}%
  \newcommand{\item}{\HlxEnterVmode%
    \begin{ifequal}{\thetemplistcounter}{\thelistcounter}%
      \HlxLeavePar\xml{/li}\end{ifequal}%
      \setcounter{templistcounter}{\thelistcounter}%
      \HlxBlk\xml{li}\HlxFirstPar}\ignorespaces}%
    {\begin{ifequal}{\thetemplistcounter}{\thelistcounter}%
        \HlxLeavePar\xml{/li}\end{ifequal}\endgroup%
    \HlxBlk\xml{/ul}\HlxModeLevelDown\resumepars}

\newenvironment{enumerate}{\endpar\suspendpars%
  \HlxBlk\HlxModeLevelUp\xml{ol}\begingroup\stepcounter{listcounter}%
  \newcounter{templistcounter}%
  \newcommand{\item}{\HlxEnterVmode%
    \begin{ifequal}{\thetemplistcounter}{\thelistcounter}%
      \HlxLeavePar\xml{/li}\end{ifequal}%
      \setcounter{templistcounter}{\thelistcounter}%
      \HlxBlk\xml{li}\HlxFirstPar}\ignorespaces}%
    {\begin{ifequal}{\thetemplistcounter}{\thelistcounter}%
        \HlxLeavePar\xml{/li}\end{ifequal}\endgroup%
    \HlxBlk\xml{/ol}\HlxModeLevelDown\resumepars}

\newenvironment{description}{\endpar\suspendpars%
  \HlxBlk\HlxModeLevelUp\xml{dl}\begingroup\stepcounter{listcounter}%
  \newcounter{templistcounter}%
  \newcommand{\item}[1][]{\HlxEnterVmode%
    \begin{ifequal}{\thetemplistcounter}{\thelistcounter}%
      \HlxLeavePar\xml{/dd}\end{ifequal}%
      \setcounter{templistcounter}{\thelistcounter}%
      \HlxBlk\HlxModeLevelUp\xml{dt}%
      \HlxEnterHmode{}##1%
      \xml{/dt}\HlxModeLevelDown\xml{dd}\HlxFirstPar}\ignorespaces}%
    {\begin{ifequal}{\thetemplistcounter}{\thelistcounter}%
        \HlxLeavePar\xml{/dd}\end{ifequal}\endgroup%
    \HlxBlk\xml{/dl}\HlxModeLevelDown\resumepars}

