\newcommand{\HlxIcons}{}

\newcommand{\BIUTopPanel}{\EmptyP{\HlxSeqPrevUrl\HlxUpUrl\HlxSeqNextUrl}{%
  \xml*{table width="100%" cellpadding=0 cellspacing=2}\xml{tr}
  \xml*{td bgcolor="##ffeab9"}%
  \EmptyP{\HlxSeqNextUrl}
  {\xlink{\HlxCallEval{\HlxImage}{\HlxPure{\HlxSeqNextTitle}}{next.gif}}%
    {\HlxSeqNextUrl}}%
  {\htmlimage[alt=""]{\HlxIcons{}blank.gif}}%
  \xml*{/td}%
  \xml*{td bgcolor="##ffeab9"}%
  \EmptyP{\HlxUpUrl}
  {\xlink{\HlxCallEval{\HlxImage}{\HlxPure{\HlxUpTitle}}{up.gif}}%
    {\HlxUpUrl}}%
  {\htmlimage[alt=""]{\HlxIcons{}blank.gif}}%
  \xml*{/td}%
  \xml*{td bgcolor="##ffeab9"}%
  \EmptyP{\HlxSeqPrevUrl}
  {\xlink{\HlxCallEval{\HlxImage}{\HlxPure{\HlxSeqPrevTitle}}{previous.gif}}%
    {\HlxSeqPrevUrl}}%
  {\htmlimage[alt=""]{\HlxIcons{}blank.gif}}%
  \xml*{/td}%
  \xml*{td align="center"  bgcolor="##ffeab9" style="color=DarkGreen" width="100%"}%
   \textbf{\Large\HlxThisTitle}%
  \xml*{/td}%
  \HlxPanelFields
  \xml*{/tr}%
  \xml*{/table}}}

\newcommand{\biupanelfield}[2]{\HlxAppend{\HlxPanelFields}%
  {\xml*{td bgcolor="###ffeab9" align="center"}\link{#1}{#2}\xml*{/td}}}

