\@open{TABLE}{CLASS="delimright"}%
\@hbarcell{\@@@barsz}%
\@bracell[ ALIGN="right"]{\@@vbar[STYLE="height:\arabic{@c}em;"]}%
\@hbarcell{\@@@barsz}%
\@close{TABLE}}%
\renewcommand{\csname\delim@name{]}\endcsname}[1]
{\@rightsqbra@table{#1}}%
