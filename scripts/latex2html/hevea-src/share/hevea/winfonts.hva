{\setcounter{@c}{(#1)}%
\@open{TABLE}{CLASS="delim"}%
\@bracell{\@print@u{X25B2}}%
\@bracell[ALIGN="center"]
{\@Mid@table{\arabic{@c}}}%
\@close{TABLE}}%
